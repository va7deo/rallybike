        <dip name="Dip Switch Display" bits="14" ids="Off,On"/>
        <dip name="Continue Play" bits="15" ids="Yes,No"/>
        <!-- SYSTEM -->
        <dip name="Test Switch" bits="18" ids="Off,On"/>
    </switches>

    <buttons names="Accelerate,Brake,-,P1 Start,P2 Start,Coin A,Coin B,Pause" default="A,B,X,Y,R,Start"/>

