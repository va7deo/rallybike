    <rbf>rallybike</rbf>
    <mameversion>0254</mameversion>
    <year>1988</year>
    <manufacturer>Toaplan / Taito Corporation</manufacturer>
    <players>2</players>
    <joystick>8-way</joystick>
    <rotation>Vertical (CW)</rotation>
    <region>Japan</region>

    <switches default="00,00,00,00,00,00,00,00">
        <!-- DSWA -->
        <dip name="Table Type" bits="0" ids="Table,Upright"/>
        <dip name="Screen Rotation" bits="1" ids="Off,On"/>
        <dip name="Service Mode" bits="2" ids="Off,On"/>
        <dip name="Advertise Sound" bits="3" ids="On,Off"/>
        <dip name="Coin 1" bits="4,5" ids="1/1,2/1,1/2,3/1,2/1,4/1,2/3"/>
